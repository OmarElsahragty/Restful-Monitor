{
  "name": "restful-monitor",
  "version": "1.0.0",
  "description": "uptime monitoring RESTful API server which allows authorized users to enter URLs they want monitored, and get detailed uptime reports about their availability, average response time, and total uptime/downtime",
  "author": "Omar Elsahragty <omar_elsahragty@hotmail.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/OmarElsahragty/Restful-Monitor"
  },
  "private": false,
  "license": "MIT",
  "scripts": {
    "docker": "NODE_ENV=Docker node -r esm .",
    "start": "NODE_ENV=Production node -r esm .",
    "dev": "NODE_ENV=Development nodemon -r esm .",
    "init": "NODE_ENV=Initialization node -r esm ./scripts/Installation",
    "rest": "NODE_ENV=Rest node -r esm ./scripts/Installation",
    "lint": "eslint ."
  },
  "dependencies": {
    "@hapi/boom": "^9.1.2",
    "bcryptjs": "^2.4.3",
    "colors": "^1.4.0",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "esm": "^3.2.25",
    "express": "^4.17.1",
    "i18next": "^19.9.1",
    "i18next-fs-backend": "^1.1.1",
    "i18next-http-middleware": "^3.1.3",
    "jsonwebtoken": "^8.5.1",
    "node-fetch": "^2.6.1",
    "nodemailer": "^6.6.3",
    "pg": "^8.5.1",
    "ping-monitor": "^0.6.1",
    "randexp": "^0.5.3",
    "redis": "^3.1.2",
    "sequelize": "^6.5.0",
    "winston": "^3.3.3",
    "yup": "^0.32.9"
  },
  "devDependencies": {
    "@babel/core": "^7.14.6",
    "@babel/eslint-parser": "^7.14.5",
    "eslint": "^7.21.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-plugin-babel": "^5.3.1",
    "eslint-plugin-import": "^2.22.1"
  }
}
